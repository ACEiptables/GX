echo "\033[32m\n启动you游追踪\033[0m"
mkdir /data/you游/
sleep 0.5
FILE=/data/you游/9999
if [ ! -f "$FILE" ]; then
echo "Hello, World!" > /data/you游/9999
    echo "\033[34m\n自动更新中\033[0m"
    curl -o /data/you游/YOU.zip "https://mirror.ghproxy.com/https://raw.githubusercontent.com/ACEiptables/GX/main/YOU.zip"
cd /data/you游/
unzip /data/you游/YOU.zip

sh 国际服.sh
    exit 1
    exit
fi
echo "欢迎使用"
cd /data/you游/
unzip /data/you游/YOU.zip

sh 国际服.sh